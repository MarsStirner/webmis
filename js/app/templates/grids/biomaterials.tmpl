<script id="biomaterials-grid" type="text/x-jquery-tmpl">
	<table class="Grid">
		<thead>
		<tr>
			<th>
				<input type="checkbox" name="all">
			</th>

			<th>
				<a class="Actions SortCol" data-field="fullName">ФИО</a>
			</th>
			<th>
				<a class="Actions SortCol" data-field="sex">Пол</a>
			</th>
			<th>
				<a class="Actions SortCol" data-field="birthDate">День рожд.</a>
			</th>
			<th>
				<a class="Actions SortCol" data-field="actionType">Тип действия</a>
			</th>
			<th>
				<a class="Actions SortCol" data-field="testTube">Пробирка</a>
			</th>
			<th>
				<a class="Actions SortCol" data-field="status">Статус</a>

			</th>
		</tr>
		</thead>
		<tbody>

		</tbody>
	</table>
</script>

<script id="biomaterials-grid-row" type="text/x-jquery-tmpl">
	<td>
		<input type="checkbox" name="bio">
	</td>

	<td>
		${fullName}
	</td>
	<td>
	${sex}
	</td>
	<td>
		{{formatDate birthDate}}

	</td>
	<td>
	${actionType}
	</td>
	<td>
		${testTube.name} ${testTube.color}
	</td>
	<td>
		${status}
	</td>
</script>

<script id="biomaterials-grid-row-error" type="text/x-jquery-tmpl">
	<td colspan="7">
		Ничего не найдено
	</td>
</script>

<script id="biomaterials-grid-row-default" type="text/x-jquery-tmpl">
	<h3>Введите необходимые данные в поля фильтров</h3>
</script>
