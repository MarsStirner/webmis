
<section class="PopUpLine">
	<div class="RefDiagVidget">
		<div class="VidgetLine">
			<div class="VidgetCol LeftCol labs-list-el" style="background-color: #eee;">

			</div>
			<div class="VidgetCol ArrowCol">
				<i>►</i>
			</div>
			<div class="VidgetCol LeftCol lab-test-list-el" style="background-color: #eee;">

			</div>
			<div class="VidgetCol ArrowCol">
				<i>►</i>
			</div>
			<div class="VidgetCol RightCol set-off-test-el">
				<ul class="edit-tree">
				</ul>
			</div>
		</div>
	</div>

	<div class="EditForm Clearfix">
		<a href="#" style="color:#fff;outline:0;">.</a>
		<!--<ul class="LineBlockHolder">-->

		<div style="margin-bottom: 1.5em;">

			<div class="QuartCol">
				<span class="Label" style="white-space: nowrap">Дата и время создания</span>
				<div class="date">


					<input type="text" data-mindate="0" name="start-date" id="start-date" placeholder="дд.мм.гггг" class="SelectDate"
								 data-relation="#start-time" />
					<i class="icon-calendar date-icon"></i>
				</div>
			</div>
			<div class="DecCol WithoutName">
				<div class="time">
					<input type="text" class="Styled HourPicker" id="start-time" data-relation="#start-date" placeholder="чч:мм"/>
					<i class="icon-time time-icon"></i>
				</div>
			</div>

			<div class="HalfCol">
				<span class="Label">Вид оплаты</span><br/>
				<select name="finance" id="finance" style="width: 100%;"></select>
			</div>
		</div>

        <div style="margin-bottom: 1.5em;">
            <div class="ColHalf">
                <span class="Label">Направивший врач</span>
                <div id="doctor-outer">
                    <input name="doctor" id="doctor" type="text" style="" disabled value="${doctor.name.first} ${doctor.name.middle} ${doctor.name.last}" class="Disabled">
                    <button class="change-doctor" style=""><i class="icon-user-md"></i></button>
                </div>
            </div>
            <div class="ColHalf">
                <span class="Label">Исполнитель</span>
                <div id="executor-outer">
                    <input name="executor" id="executor" type="text" style="" disabled value="" class="Disabled">
                    <button class="change-executor" style=""><i class="icon-user-md"></i></button>
                </div>
            </div>
        </div>

		<div class="mbk">мбк</div>

	</div>

</section>





