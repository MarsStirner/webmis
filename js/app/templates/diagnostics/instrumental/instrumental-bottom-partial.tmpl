<div class="row-fluid">
    <div class="span2">
        <span class="Label" style="white-space: nowrap">Дата и время создания</span>

        <div class="date">
            <input
            type="text"
            data-mindate="0"
            name="createDate"
            id="createDate"
            placeholder="дд.мм.гггг"
            class="SelectDate"/>
            <i class="icon-calendar date-icon"></i>
        </div>
    </div>

    <div class="span1 WithoutName">
        <div class="time">
            <input
            type="text"
            class="Styled HourPicker"
            id="createTime"
            placeholder="чч:мм"/>
            <i class="icon-time time-icon"></i>
        </div>
    </div>


    <div class="span3">
        <span class="Label">Вид оплаты</span><br/>
        <select name="finance" id="finance" style="width: 100%;"></select>
    </div>
</div>
<div class="row-fluid" style="margin-bottom: 1.5em;">
    <div class="span6">
        <span class="Label">Направивший врач</span>

        <div id="assigner-outer">
            <input
            name="assigner"
            id="assigner"
            type="text"
            style=""
            disabled
            value="${assignerLastName} ${assignerFirstName} ${assignerMiddleName} "
            class="Disabled">
            <button class="change-assigner" style=""><i class="icon-user-md"></i></button>
        </div>
    </div>
    <div class="span6">
        <span class="Label">Исполнитель</span>

        <div id="executor-outer">
            <input
            name="executor"
            id="executor"
            type="text"
            style=""
            disabled
            value="${executorLastName} ${executorFirstName} ${executorMiddleName} "
            class="Disabled">
            <button class="change-executor" style=""><i class="icon-user-md"></i></button>
        </div>
    </div>
</div>
<div class="mkb"></div>

