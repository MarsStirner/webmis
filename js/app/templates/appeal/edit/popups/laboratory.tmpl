
<section class="PopUpLine labs-popup">
	<div class="RefDiagVidget">
		<div class="VidgetLine">
			<div class="VidgetCol LeftCol labs-list-el">

			</div>
			<div class="VidgetCol ArrowCol">
				<i>►</i>
			</div>
			<div class="VidgetCol LeftCol lab-test-list-el">

			</div>
			<div class="VidgetCol ArrowCol">
				<i>►</i>
			</div>
			<div class="VidgetCol RightCol set-off-test-el">
				<ul class="SelectAnalysis">

				</ul>
			</div>
		</div>
	</div>

	<div class="EditForm Clearfix"><a href="#" style="color:#fff;outline:0;">.</a>

		<div style="margin-bottom: 1.5em;">
			<div class="QuartCol">
				<span class="Label" style="white-space: nowrap">Дата и время создания</span>
				<div class="date">


					<input type="text" data-mindate="0" name="start-date" id="start-date" placeholder="дд.мм.гггг" class="SelectDate"
								 data-relation="#start-time" />
					<i class="icon-calendar date-icon"></i>
				</div>
				<!--<div class="DateTime">
					<div class="DatePeriod SingleDate">
						<div class="FromTo">
							<input type="text" data-mindate="0" name="start-date" id="start-date" placeholder="дд.мм.гггг" class="SelectDate"
										 data-relation="#start-time"/>
						</div>
						<i class="DateIcon Icon"></i>
					</div>
					<div class="SemiCol">
						<input type="text" class="Styled HourPicker" id="start-time" data-relation="#start-date" placeholder="чч:мм"/>
					</div>

				</div>-->
			</div>
			<div class="DecCol WithoutName">
				<div class="time">
					<input type="text" class="Styled HourPicker" id="start-time" data-relation="#start-date" placeholder="чч:мм"/>
					<i class="icon-time time-icon"></i>
				</div>
			</div>
			<div class="ColHalf">
				<span class="Label">Направивший врач</span>
				<input name="doctor" type="text" style="color: #777;" disabled value="${doctor.name.first} ${doctor.name.last}">
			</div>
			<div class="HalfCol">
				<span class="Label">Вид оплаты</span><br/>
				<select name="finance" id="finance" style="width: 100%;"></select>
			</div>
		</div>

		<div class="LineBlock">
			<div class="QuartCol">
				<span class="Label" style="margin-left: 0;">Диагноз</span>
				<input name="diagnosis[mkb][code]" class="ExamAttr"
							 type="text" value="">
				<span class="FieldInfo">Код по МКБ</span>
			</div>
			<div class="HugeCol WithoutName">
				<input name="diagnosis[mkb][diagnosis]" class="Disabled NotEditable" disabled="disabled" type="text"
							 placeholder="" value=""/>
				<span class="FieldInfo">Описание</span>
			</div>
			<div class="QuartCol" style="padding-top: 1.9em;">
				<button class="MKBLauncher" >МКБ-10</button>
			</div>
		</div>

	</div>

