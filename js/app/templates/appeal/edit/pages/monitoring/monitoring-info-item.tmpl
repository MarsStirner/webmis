<%if (collection.length) {%>
	<%collection.each(function (item) {%>
		<tr>
			<td><%=Core.Date.formatDateTime(item.get("datetime"))%></td>
			<td><%=item.get("temperature")%></td>
			<td><%=item.get("bloodPressure").right.syst%>/<%=item.get("bloodPressure").right.diast%></td>
			<td><%=item.get("heartRate")%></td>
			<td><%=item.get("spo2")%></td>
			<td><%=item.get("breathRate")%></td>
			<td><%=item.get("state")%></td>
			<td><%=item.get("health")%></td>
		</tr>
	<%});%>
<%} else {%>
	<tr>
		<td colspan="8">Нет данных</td>
	</tr>
<%}%>